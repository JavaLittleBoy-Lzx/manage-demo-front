# 停车管理系统环境变量配置示例
# 复制此文件为 .env.local 并根据实际情况修改配置

# 应用基础配置
NODE_ENV=development
VUE_APP_TITLE=停车管理系统
VUE_APP_VERSION=1.0.0

# API配置
VUE_APP_API_BASE_URL=https://www.xuerparking.cn:8543
VUE_APP_API_TIMEOUT=5000000

# BigModel AI配置
# 请在智谱AI官网申请API密钥: https://open.bigmodel.cn/
VUE_APP_BIGMODEL_API_KEY=your_bigmodel_api_key_here
VUE_APP_BIGMODEL_BASE_URL=https://open.bigmodel.cn/api/paas/v4
VUE_APP_BIGMODEL_TIMEOUT=60000

# BigModel功能开关
VUE_APP_ENABLE_AI_CUSTOMER_SERVICE=true
VUE_APP_ENABLE_AI_VIOLATION_DESCRIPTION=true
VUE_APP_ENABLE_AI_DATA_REPORT=true
VUE_APP_ENABLE_AI_NOTIFICATION_TEXT=true
VUE_APP_ENABLE_AI_BEHAVIOR_ANALYSIS=false

# 文件上传配置
VUE_APP_UPLOAD_MAX_SIZE=10485760
VUE_APP_UPLOAD_ALLOWED_TYPES=image/jpeg,image/png,image/gif
VUE_APP_UPLOAD_URL=/api/upload

# 地图配置（如需要）
VUE_APP_MAP_API_KEY=your_map_api_key_here

# 日志配置
VUE_APP_ENABLE_DEBUG_LOG=true
VUE_APP_LOG_LEVEL=info

# 缓存配置
VUE_APP_CACHE_ENABLED=true
VUE_APP_CACHE_TTL=1800000

# 会话配置
VUE_APP_SESSION_TIMEOUT=600000
VUE_APP_SESSION_WARNING_TIME=300000

# 分页配置
VUE_APP_DEFAULT_PAGE_SIZE=20
VUE_APP_MAX_PAGE_SIZE=100

# 主题配置
VUE_APP_PRIMARY_COLOR=#409EFF
VUE_APP_SUCCESS_COLOR=#67C23A
VUE_APP_WARNING_COLOR=#E6A23C
VUE_APP_DANGER_COLOR=#F56C6C

# 开发环境特定配置
VUE_APP_DEV_MOCK_ENABLED=false
VUE_APP_DEV_PROXY_ENABLED=true
VUE_APP_DEV_PROXY_TARGET=https://www.xuerparking.cn:8543

# 生产环境特定配置
VUE_APP_PROD_MINIFIED=true
VUE_APP_PROD_SOURCE_MAP=false
VUE_APP_PROD_CDN_ENABLED=false

# 第三方服务配置
VUE_APP_SENTRY_DSN=your_sentry_dsn_here
VUE_APP_ANALYTICS_ID=your_analytics_id_here

# 邮件服务配置
VUE_APP_SMTP_HOST=smtp.example.com
VUE_APP_SMTP_PORT=587
VUE_APP_SMTP_USER=noreply@example.com
VUE_APP_SMTP_PASS=your_smtp_password

# 短信服务配置
VUE_APP_SMS_PROVIDER=aliyun
VUE_APP_SMS_ACCESS_KEY=your_sms_access_key
VUE_APP_SMS_SECRET_KEY=your_sms_secret_key